#python language used

class Node:
  def __init__(self, data, next=None):
    self.data = data
    self.next = next
    
class NodeManag:
  def __init__(self, data):
    self.head = Node(data)
  
  def add(data): #add new data
    if self.head=='':
      self.head = Node(data)
    else:
      node = self.head
      while node.next
        node = node.next
      node.next = Node(data)
      
   def desc(self):
    node = self.head
    while node:
      print(node.data)
      node = node.next
    
    def delete(self,data):
      if self.head=='':
        print("There is no Node")
        return
      if self.head.data==data:
        temp=self.head
        self.head=self.head.next
        del temp
      else:
        node=self.head
        while node.next:
          if node.next.data==data:
            temp=node.next
            node.next=node.next.next
            del temp
            return
           else:
            node=node.next
            
     def search_from_head(self,data):
      node=self.head
      while node:
        if node.data==data:
          return node
        else:
          node=node.next
        return
        
    
linkedlist1= NodeManage(0)
linkedlist.desc()

for data in range (1, 10)
  linkedlist1.add(data)
linkedlist1.desc()

