#python language used

class Node:
  def __init__(self, data, next=None):
    self.data = data
    self.next = next
    
class NodeManag:
  def __init__(self, data):
  self.head = Node(data)
  
  def add(data): #add new data
    node = head
    while = node.next:
      node = node.next
     node.next = Node(data)
     
node1 = Node(1)
node2 = Node(2)
node.next = node2
head = node1
for index in range(2, 10):
  add(index)

node3 = Node(1.5)

node = head
while node.next: #loop till the node.next excist
  print(node.data)
  node = node.next
print(node.data)

node_next = node.next
node.next = node3
node3.next = node_next

