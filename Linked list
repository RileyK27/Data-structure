#python language used

class Node:
  def __init__(self, data, next=None):
    self.data = data
    self.next = next
    
class NodeManag:
  def __init__(self, data):
    self.head = Node(data)
  
  def add(data): #add new data
    if self.head==' ':
      self.head = Node(data)
    else:
      node = self.head
      while node.next
        node = node.next
      node.next = Node(data)
      
   def desc(self):
    node = self.head
    while node:
      print(node.data)
      node = node.next
     
linkedlist1= NodeManage(0)
linkedlist.desc()

for data in range (1, 10)
  linkedlist1.add(data)
linkedlist1.desc()

